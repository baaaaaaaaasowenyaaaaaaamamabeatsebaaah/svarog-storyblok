{
  "name": "svarog-storyblok-site",
  "version": "1.0.0",
  "description": "Website built with Svarog component library and Storyblok CMS",
  "main": "server.mjs",
  "type": "module",
  "scripts": {
    "dev": "webpack serve --mode development --config webpack.config.cjs",
    "build": "webpack --mode production --config webpack.config.cjs",
    "start": "node server.mjs",
    "start:prod": "node server.mjs",
    "start:dev": "npm run dev",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.{js,css,html}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@storyblok/js": "^3.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "storyblok-js-client": "^6.7.1",
    "svarog-ui": "^1.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@eslint/eslintrc": "^2.1.4",
    "@eslint/js": "^8.57.0",
    "ajv": "^8.12.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.10.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^13.24.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^8.0.3",
    "jsdom": "^24.0.0",
    "mini-css-extract-plugin": "^2.8.1",
    "prettier": "^3.2.5",
    "style-loader": "^3.3.4",
    "vitest": "^1.4.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "keywords": [
    "svarog",
    "storyblok",
    "cms",
    "javascript",
    "components"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.18.0"
  }
}
